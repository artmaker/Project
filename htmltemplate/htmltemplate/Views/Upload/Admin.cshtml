


@model IEnumerable<htmltemplate.Models.UserInformation>
@{
    ViewBag.Title = "Admin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <title>HTML Education Template</title>

    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Lato:700%7CMontserrat:400,600" rel="stylesheet">

    <!-- Bootstrap -->
    <link type="text/css" rel="stylesheet" href=@Url.Content("~/contents/css/bootstrap.min.css") />

    <!-- Font Awesome Icon -->
    <link rel="stylesheet" href=@Url.Content("~/contents/css/font-awesome.min.css")>

    <!-- Custom stlylesheet -->
    <link type="text/css" rel="stylesheet" href=@Url.Content("~/contents/css/mainstyle.css") />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body>

    <!-- Header -->
    <!-- /Header -->
    <!-- Hero-area -->
    <!-- /Hero-area -->
    <!-- Contact -->
    <div id="contact" class="section">

        <!-- container -->
        <div class="container">

            <!-- row -->
            <div class="row">

                <!-- contact form -->
                <div class="col-md-6">
                    <h4>Ideas</h4>
                    <div class="container-table100">
                        <div class="wrap-table100">
                            <div class="table">

                                <div class="row header">
                                    <div class="cell">
                                        @Html.DisplayNameFor(model => model.Name)
                                    </div>
                                    <div class="cell">
                                        @Html.DisplayNameFor(model => model.Email)
                                    </div>
                                    <div class="cell">
                                        @Html.DisplayNameFor(model => model.UserType)
                                    </div>
                                    <div class="cell">
                                        @Html.DisplayNameFor(model => model.TeacherId)
                                    </div>
                                    <div class="cell">
                                        Actions
                                    </div>
                                </div>
                                @foreach (var item in Model)
                                {
                                    <div class="row">
                                        <div class="cell" data-title="Full Name">
                                            @Html.DisplayFor(modelItem => item.Name)
                                        </div>
                                        <div class="cell" data-title="Age">
                                            @Html.DisplayFor(modelItem => item.Email)
                                        </div>
                                        <div class="cell" data-title="Job Title">
                                            @Html.DisplayFor(modelItem => item.UserType)
                                        </div>
                                        <div class="cell" data-title="Job Title">
                                            @Html.DisplayFor(modelItem => item.TeacherId)
                                        </div>
                                        <div class="cell" data-title="Location">

                                            @Html.ActionLink("ApproveUser", "AcceptUser", new { ide = item.Email }, new { @style = "color:#000aaa;" }) |
                                            @Html.ActionLink("RejectUser", "RejectUser", new { ide = item.Email }, new { @style = "color:#FF0000;" })




                                        </div>
                                    </div>
                                }







                            </div>
                            <!-- <button class="main-button icon-button pull-left">Upload</button>-->
                        </div>
                    </div>

                </div>


                <!-- /contact form -->
                <!-- contact information -->
                <!-- contact information -->

            </div>
            <!-- /row -->

        </div>
        <!-- /container -->

    </div>
    <!-- /Contact -->
    <!-- Footer -->
    <!-- /Footer -->
    <!-- preloader -->
    <!-- /preloader -->
    <!-- jQuery Plugins -->
    <script type="text/javascript" src=@Url.Content("~/contents/js/jquery.min.js")></script>
    <script type="text/javascript" src=@Url.Content("~/contents/js/bootstrap.min.js")></script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>
    <script type="text/javascript" src=@Url.Content("~/contents/js/google-map.js")></script>
    <script type="text/javascript" src=@Url.Content("~/contents/js/main.js")></script>

</body>
</html>


